{
    "name": "cycling-around",
    "version": "1.0.0",
    "description": "An application to estimate cycling times to various tourist destinations.",
    "main": "src/app.js",
    "scripts": {
        "start": "node app.js",
        "dev": "nodemon app.js",
        "db:init": "sequelize-cli db:create",
        "db:migrate": "sequelize-cli db:migrate",
        "db:seed": "sequelize-cli db:seed:all",
        "db:clean": "sequelize-cli db:seed:undo:all",
        "db:delete": "sequelize-cli db:drop"
    },
    "dependencies": {
        "bcryptjs": "^2.4.3",
        "dotenv": "^16.3.1",
        "express": "^4.18.2",
        "jsonwebtoken": "^9.0.2",
        "mysql2": "^3.6.2",
        "sequelize": "^6.33.0"
    },
    "devDependencies": {
        "nodemon": "^3.0.1",
        "sequelize-cli": "^6.6.1"
    },
    "author": "Adarsh Kumar",
    "website": "https://CyclingAround.adarshkumar.me",
    "git": "https://github.com/idealadarsh/CyclingAround",
    "license": "MIT"
}